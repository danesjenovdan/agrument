    "passport-local": "^1.0.0",
    "password-hash-and-salt": "^0.1.4",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "randomstring": "^1.1.5",
    "react": "^16.13.1",
    "react-date-picker": "^6.12.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-rte": "^0.16.1",
    "react-select": "^2.0.0",
    "react-sticky": "^6.0.3",
    "react-textarea-autosize": "^7.1.2",
    "react-timeago": "^4.4.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
