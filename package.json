{
  "name": "agrument-react",
  "version": "1.0.0",
  "private": true,
  "description": "agrument - react",
  "scripts": {
    "start": "webpack-dev-server --progress --config webpack.config.js",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "build:server": "rimraf dist_server && cross-env NODE_ENV=production babel src -d dist_server",
    "build": "npm run build:client && npm run build:server",
    "migrate": "knex migrate:latest",
    "serve": "node ./dist_server/server",
    "build-serve": "npm run build && cross-env PORT=8005 npm run serve",
    "api": "npm run build:server && cross-env PORT=8080 npm run serve",
    "lint": "eslint ./seeds ./migrations ./src",
    "deploy:setup": "cross-env NODE_ENV=production pm2 deploy production setup",
    "deploy": "cross-env NODE_ENV=production pm2 deploy production"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "connect-session-knex": "^1.4.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "draft-js-utils": "^1.2.0",
    "draftjs-filters": "^1.0.0",
    "express": "^4.16.3",
    "express-error-slack": "^1.0.2",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "freezer-js": "^0.14.0",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-type": "^3.0.0",
    "knex": "^0.15.2",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-hash-and-salt": "^0.1.4",
    "prop-types": "^15.6.2",
    "querystring": "^0.2.0",
    "randomstring": "^1.1.5",
    "react": "^16.4.1",
    "react-date-picker": "^6.11.2",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-rte": "^0.16.1",
    "react-select": "^2.0.0",
    "react-sticky": "^6.0.3",
    "react-textarea-autosize": "^7.0.4",
    "react-timeago": "^4.1.9",
    "react-waypoint": "^8.0.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "slack-node": "^0.1.8",
    "sqlite3": "^4.0.2",
    "style-loader": "^0.21.0",
    "superagent": "^3.8.3",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1"
  }
}
