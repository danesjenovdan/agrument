    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.15.0",
    "babel-runtime": "^6.16.0",
    "classnames": "^2.2.5",
    "core-decorators": "^0.15.0",
    "cropperjs": "^0.8.1",
    "cross-env": "^3.1.4",
