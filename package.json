{
  "name": "djnd-react",
  "version": "1.0.0",
  "private": true,
  "description": "Danes je nov dan - React",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "rimraf dist_server && cross-env NODE_ENV=production babel src -d dist_server",
    "build": "npm run build:client && npm run build:server",
    "serve": "node ./dist_server/server",
    "build-serve": "npm run build && npm run serve"
  },
