{
  "name": "agrument-react",
  "version": "1.0.0",
  "private": true,
  "description": "agrument - react",
  "scripts": {
    "start": "webpack-dev-server --progress --config webpack.config.js",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "build:server": "rimraf dist_server && cross-env NODE_ENV=production babel src -d dist_server",
    "build": "npm run build:client && npm run build:server",
    "serve": "node ./dist_server/server",
    "build-serve": "npm run build && cross-env PORT=8005 npm run serve",
    "api": "npm run build:server && cross-env PORT=8080 npm run serve",
    "lint": "eslint src"
  },
  "dependencies": {
