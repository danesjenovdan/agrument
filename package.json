{
  "name": "agrument-react",
  "version": "1.0.0",
  "private": true,
  "description": "agrument - react",
  "scripts": {
    "start": "webpack-dev-server --progress --config webpack.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "build:server": "rimraf dist_server && cross-env NODE_ENV=production babel src -d dist_server",
    "build": "npm run build:client && npm run build:server",
    "migrate": "knex migrate:latest",
    "serve": "node ./dist_server/server",
    "build-serve": "npm run build && cross-env PORT=8005 npm run serve",
    "api": "npm run build:server && cross-env PORT=8080 npm run serve",
    "api:dev": "cross-env PORT=8080 nodemon --watch ./src --exec babel-node ./src/server",
    "lint": "eslint ./seeds ./migrations ./src",
    "deploy:setup": "cross-env NODE_ENV=production pm2 deploy production setup",
    "deploy": "cross-env NODE_ENV=production pm2 deploy production"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "body-parser": "^1.19.0",
    "brute-knex": "^4.0.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "connect-session-knex": "^1.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "draft-js-utils": "^1.2.0",
    "draftjs-filters": "^1.0.0",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-error-slack": "^2.0.0",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "feed": "^4.1.0",
    "file-loader": "^6.0.0",
    "freezer-js": "^0.14.1",
    "fs-extra": "^9.0.0",
